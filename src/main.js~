export const eventBus = new Vue();

let vm=new Vue({
  el: '#app',
  render: h => h(App),
  created() {
  	  console.log("store "+this.$store.getters.users)
     console.log("CREATED: this.data.user.name: ");  
  },
  mounted() {
  	  console.log("store "+this.$store.getters.users)
  	  console.log("MOUNTED: this.data.user.name: ");
  },
  updated() {
  	  console.log("store "+this.$store.getters.users)
     console.log("UPATED: this.data.user.name: ");  
  },
  store
})
